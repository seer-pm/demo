{
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "moduleResolution": "node", 
    "resolveJsonModule": true,
    "allowImportingTsExtensions": true,
    "target": "ES2020",
    "noEmit": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "paths": {
      "@/*": ["../src/*"],
    },
  },
  "include": ["./functions/**/*", "../src/hooks/queries/**/*", "../src/hooks/contracts/**/*", "../src/lib/chains.ts", "../src/lib/utils.ts"]
}



