{
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "moduleResolution": "node", 
    "resolveJsonModule": true,
    "allowImportingTsExtensions": true,
    "target": "ES2020",
    "noEmit": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noImplicitAny": true
  },
  "include": ["./functions/**/*"]
}



